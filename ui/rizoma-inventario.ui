                <property name="n_rows">2</property>
                <property name="n_columns">2</property>
                <property name="column_spacing">9</property>
                <property name="row_spacing">16</property>
                <child>
                  <object class="GtkButton" id="btn_ejecutar_Inv">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="receives_default">True</property>
                    <property name="label" translatable="yes">gtk-execute</property>
                    <property name="use_stock">True</property>
                    
                    <signal name="clicked" handler="on_btn_ejecutar_Inv"/>
                  </object>
                  <packing>
                    <property name="left_attach">1</property>
                    <property name="right_attach">2</property>
                    <property name="top_attach">1</property>
                    <property name="bottom_attach">2</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkEntry" id="direccion_entry">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="editable">False</property>
                    <signal name="activate" handler="on_btn_abrir_clicked"/>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="label2">
                    <property name="visible">True</property>
                    <property name="label" translatable="yes">Realiza Inventario</property>
                  </object>
                  <packing>
                    <property name="top_attach">1</property>
                    <property name="bottom_attach">2</property>
                  </packing>
                </child>
                <child>
                  <object class="GtkButton" id="btn_abrir">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="receives_default">True</property>
                    <property name="label" translatable="yes">gtk-open</property>
                    <property name="use_stock">True</property>
                    
                    <signal name="clicked" handler="on_btn_abrir_clicked"/>
                  </object>
                  <packing>
                    <property name="left_attach">1</property>
                    <property name="right_attach">2</property>
                  </packing>
                </child>
              </object>
            </child>
            <child>
